{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "title": "create user",
    "additionalProperties": false,
    "required": ["username", "email", "id"],
    "properties": {
      "id": {
        "type": "number"
      },
      "username": {
        "type": "string",
        "minLength": 3,
        "maxLength": 32
      },
      "email": {
        "type": "string",
        "format": "email",
        "pattern": "^\\S+@\\S+\\.\\S+$"
      }
  }
}
  